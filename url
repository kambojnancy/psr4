<?php

class User {
  const DEFAULT_USER_NAME = 'JOHN';
  
  private $name;

  public function getNameOfTheUser()
  {
      return $this->name;
  }

  public function setNameOfTheUser($name)
  {
      $this->name = $name;
  }

}


$user = new User;
$user->setNameOfTheUser('Avantika');
echo $user->getNameOfTheUser();



<?php

class User {

  public function __construct()
  {
      echo __CLASS__ . " Constructor called \n";
  }

  public function getNameOfTheUser()
  {
      return $this->name;
  }

  public function setNameOfTheUser($name)
  {
      $this->name = $name;
  }

  public function __destruct()
  {
      echo __CLASS__ . " Destructor called \n";
  }

}


$user = new User;
$user->setNameOfTheUser('Avantika');
echo $user->getNameOfTheUser() . "\n";



<?php
namespace Home;
class Work {
  public function getTask()
  {
      return 'Goto store and buy cookies';
  }
}

namespace Office;
class Work {
  public function getTask()
  {
      return 'Work on PHP project';
  }
}

$work = new \Office\Work;
echo $work->getTask();



